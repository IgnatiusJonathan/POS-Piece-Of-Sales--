// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Worker{
  id  Int @id @default(autoincrement())
  nama  String
  email String
  password String
}

model Product{
  id  Int @id @default(autoincrement())
  nama String
  jenis String
  stok Int
  harga Int
  image String?
}

model History{
  id  Int @id @default(autoincrement())
  product String
  jumlah Int
  harga Int
  tanggal DateTime 
  pembeli String
  kasier String
}

model Member{
  id  Int @id @default(autoincrement())
  name String
  tanggal DateTime 
  noTelp  Int
  email String
}

