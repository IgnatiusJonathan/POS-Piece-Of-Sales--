// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Worker{
  id  int @id @default(autoincrement())
  nama  String
  email String
  password String
}

model Product{
  id  int @id @default(autoincrement())
  nama String
  jenis String
  stok int
  harga int
  image String?
}

model History{
  id  int @id @default(autoincrement())
  product String
  jumlah int
  harga int
  tanggal date
  pembeli String
  kasier String
}

model Member{
  id  int @id @default(autoincrement())
  name String
  tanggal date
  noTelp  int
  email String
}

